<UserControl x:Class="Greenshot2.Editor.Ui.Views.CanvasView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Greenshot2.Editor.Ui.Views"
             xmlns:shapes="clr-namespace:Greenshot2.Editor.Shapes;assembly=Greenshot2.Editor.Shapes"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Canvas Name="DrawingCanvas">
        <shapes:Arrow X1="70" Y1="200" X2="100" Y2="100" Stroke="Black" Fill="DarkCyan" />
        <shapes:Arrow X1="30" Y1="300" X2="300" Y2="30"/>
    </Canvas>
</UserControl>
